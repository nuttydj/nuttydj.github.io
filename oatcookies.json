{
  "date": "29/10/2020",
  "location": "master",
  "version": 1,
    "ingredients" : [
    {
      "ingredient": "Golden Syrup",
      "amount": "2 tbsp"
    },
    
    {
      "ingredient": "Sugar",
      "amount": "6oz"
    },
    
    {
      "ingredient": "Baking Powder",
      "amount": "1 tsp"
    },
    
    {
      "ingredient": "Bicarbonate Soda (or 3 tsp Baking powder)",
      "amount": "1 tsp"
    },
    
    {
      "ingredient": "Self Raising Flour",
      "amount": "6oz"
    },
    
    {
      "ingredient": "Butter",
      "amount": "6oz"
    }
  ],
  "steps": [
      {
      "instruction": "Microwave butter and golden syrup until butter is melted"
      },
      
      {
      "instruction": "Add dry ingredients and mix thoroughly"
      },
    
      {
      "instruction": "Roll into small lumps of mix and press slightly onto baking tray"
      },
    
      {
      "instruction": "Cook at 160 degress C for 15 minutes"
      }
  ]
}
